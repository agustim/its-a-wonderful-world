{
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "REACT_APP_GRAPHQL_ENDPOINT": "@graphql-endpoint",
      "REACT_APP_PUSHER_ENDPOINT": "@pusher-endpoint",
      "REACT_APP_PUSHER_KEY": "@pusher-key",
      "REACT_APP_PUSHER_CLUSTER": "@pusher-cluster"
    }
  }
}